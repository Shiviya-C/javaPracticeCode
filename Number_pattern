import java.util.*;
class Pattern {
    public static void main(String[] args) {
        Scanner obj = new Scanner(System.in);
        int n = obj.nextInt();
        for(int i=1;i<=n;i++){
            for(int j=0;j<n;j++){
                if((i+j)>n){
                    System.out.print((i+j)-n);
                }
                else{
                    System.out.print(i+j);
                }   
            }
            System.out.println();
        }
    }
}
